<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>cve__TurnAroundTime__c</fullName>
    <deprecated>false</deprecated>
    <externalId>false</externalId>
    <formula>cve__InitialDecision__c - 
IF($Setup.cve__Switches__c.cve__TurnAroundTimeBasedOnAllDocsReceived__c,
    cve__Claim__r.cve__AllRequiredDocumentsReceived__c,
    IF($Setup.cve__Switches__c.cve__TurnAroundTimeBasedOnDateReceived__c, 
        cve__DateReceived__c, 
        cve__InitialAssignment__c
